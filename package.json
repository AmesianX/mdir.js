{
  "name": "mdir.js",
  "version": "0.5.17-alpha+024",
  "description": "Mdir.js is visual file manager. (text mode application)",
  "main": "./build/main.js",
  "scripts": {
    "clean": "rm -rf ./build",
    "build": "tsc -p tsconfig.json",    
    "start": "tsc -p tsconfig.json && node ./build/main.js",
    "debug": "tsc -p tsconfig.json && node --inspect-brk ./build/main.js",
    "test": "NODE_ENV=test tsc -p tsconfig.json && node ./build/test/test.js",
    "test2": "tsc -p tsconfig.json && node ./build/test/imagetest.js",
    "prepare": "tsc -p tsconfig.json",
    "postinstall": "node ./scripts/post-install.js",
    "pkg": "pkg --target=node12-macos-x64 --out-path=./binary .",
    "pkg-win": "pkg --debug --target=node12-win-x64 --out-path=./binary ."
  },
  "author": "La Byoungyoung <la9527@daum.net>",
  "contributors": [
    {
      "name": "La Byoungyoung",
      "email": "la9527@daum.net"
    }
  ],
  "bin": {
    "mdir": "bin/mdir"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/la9527/mdir.js.git"
  },
  "license": "BSD3",
  "keywords": [
    "MDir",
    "MC",
    "filemanager",
    "LinM",
    "CUI",
    "Xterm"
  ],
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "drivelist": "^9.0.2",
    "file-type": "^14.6.2",
    "fswin": "^3.18.918",
    "i18next": "^19.5.1",
    "i18next-cli-language-detector": "^1.0.0",
    "i18next-fs-backend": "^1.0.6",
    "moment": "^2.26.0",
    "neo-blessed": "^0.2.0",
    "node-pty": "^0.9.0",
    "sprintf-js": "^1.1.2",
    "term.js": "0.0.7",
    "which": "^2.0.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0",
    "xterm": "^4.7.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/drivelist": "^6.4.1",
    "@types/node": "^11.10.4",
    "@types/sprintf-js": "^1.1.2",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/parser": "^3.0.0",
    "pkg": "^4.4.8",
    "typescript": "^3.9.3"
  },
  "pkg": {
    "scripts": [
      "build/**/*.js"
    ],
    "assets": [
      "node_modules/neo-blessed/**",
      "node_modules/node-pty/**",
      "node_modules/drivelist/**"
    ],
    "tagets": [
      "node12"
    ]
  }
}
