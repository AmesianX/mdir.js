{
  "name": "mdir.js",
  "version": "0.2.2-alpha+007",
  "description": "Mdir.js is visual file manager. (text mode application)",
  "main": "./build/main.js",
  "scripts": {
    "clean": "rm -rf ./build",
    "build": "tsc -p tsconfig.json",
    "start": "tsc -p tsconfig.json && node ./build/main.js",
    "debug": "tsc -p tsconfig.json && node --inspect-brk ./build/main.js",
    "test": "tsc -p tsconfig.json && node ./build/test/test.js"
  },
  "author": "La Byoungyoung <la9527@daum.net>",
  "contributors": [
    {
      "name": "La Byoungyoung",
      "email": "la9527@daum.net"
    }
  ],
  "bin": {
    "mdir": "bin/mdir.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/la9527/mdir.js.git"
  },
  "license": "BSD3",
  "keywords": [
    "MDir",
    "MC",
    "filemanager",
    "LinM",
    "CUI",
    "Xterm"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "drivelist": "^9.0.2",
    "fswin": "^3.18.918",
    "moment": "^2.26.0",
    "neo-blessed": "^0.2.0",
    "node-pty": "^0.9.0",
    "sprintf-js": "^1.1.2",
    "term.js": "0.0.7",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0",
    "xterm": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^11.10.4",
    "@types/sprintf-js": "^1.1.2",
    "@typescript-eslint/parser": "^3.0.0",
    "@types/drivelist": "^6.4.1",
    "typescript": "^3.9.3"
  }
}
